  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "run vue3",
      "port": 9222,
      "sourceMaps": true,
      "url": "http://localhost:8080/",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///./src/*": "${webRoot}/*"
      },
      "webRoot": "${workspaceFolder}/src"
    },


