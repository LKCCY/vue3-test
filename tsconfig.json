{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "./dist/",
    "downlevelIteration": true,
    // "typeRoots" : [
    //   "./temp"
    // ]
  },
  "exclude": [
    "node_modules"
  ]
}
